{
    "_version": "1.48.0",
    "sap.app": {
        "id": "com.app.capacitymanagement",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.5",
            "toolsId": "03855b9d-f521-4556-bccf-e697b3512651"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZEWM_CAPACITYMANAGMENT_SRV/",                                       
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/mainService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.108.11",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.app.capacitymanagement.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.app.capacitymanagement.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMainPage",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMainPage"
                    ]
                },
                {
                    "name": "RouteSimulationDetails",
                    "pattern": "SimulationDetails/:simID:",
                    "target": [
                      "TargetSimulationDetails"
                    ]
                  },
                  {
                    "name": "RouteManualSimulationDetails",
                    "pattern": "ManualSimulationDetails/:simulationName:",
                    "target": [
                      "TargetManulaSimulationDetails"
                    ]
                  }
            ],
            "targets": {
                "TargetMainPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "MainPage",
                    "viewName": "MainPage"
                },
                "TargetSimulationDetails": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "SimulationDetails",
                    "viewName": "SimulationDetails"
                  },
                  "TargetManulaSimulationDetails": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "manualsimulation",
                    "viewName": "manualsimulation"
                  }
            }
        },
        "rootView": {
            "viewName": "com.app.capacitymanagement.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}