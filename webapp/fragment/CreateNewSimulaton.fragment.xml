<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="Create new simulation" id="createSimulationDialog"   resizable="true"
        draggable="true">
        <VBox id="_IDGenVBox1">
            <Text id="_IDGenText1" class="sapUiTinyMargin" text="Simulation Name:"/>
            <Input id="simulationInput" placeholder="Enter Simulation Name" editable="false" />
        </VBox>
        <beginButton>
            <Button id="_IDGenButton" text="Submit" press="onSubmitSimulation"/>
        </beginButton>
        <endButton>
            <Button id="_IDGenButton1" text="Close" press="onCloseDialogSimulate"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>