<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Dialog
        id="idEditProductDssialog"
        resizable="true"
        draggable="true"
        title="Edit Product"
    >
        <beginButton>
            <Button
                id="idCreatssevgbhjkButton"
                text="Save"
                press="onSaveProduct"
                type="Emphasized"
            />
        </beginButton>
        <endButton>
            <Button
                id="idCancesslbnmButton"
                text="Cancel"
                press="onCloseEditModel"
                type="Emphasized"
            />
        </endButton>
        <content>
            <form:SimpleForm
                id="idStarDesstanmilsForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="3"
                labelSpanL="3"
                labelSpanM="3"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
            >
                <VBox>
                    <HBox>
                        <Label
                            id="editProductNoLabel"
                            text="Product Number"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editProductNoInput"
                            value="{CombinedModel>/Product/model}"
                            editable="false"
                            width="10rem"
                        />
                    </HBox>
                    <HBox>
                        <Label
                            id="editDescriptionLabel"
                            text="Description"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editDescriptionInput"
                            value="{CombinedModel>/Product/description}"
                            editable="false"
                            width="10rem"
                        />
                    </HBox>

                    <!-- <Label
                    id="editQuantityabel"
                    text="Quantity"
               <Label
                    id="editQuantityabel"
                    text="Quantity"
                />
                <Input
                    id="editQuantityInput"
                   value="{CombinedModel>/Product/quantity}"
                />  -->
                    <HBox>
                        <Label
                            id="editstackLabel"
                            text="Stack"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editstackInput"
                            value="{CombinedModel>/Product/stack}"
                            liveChange="onEidtStack_LC"
                            type="Number"
                            width="5rem"
                        />
                    </HBox>
                    <!-- <Label
                    id="editwuomLabel"
                    text="wuom"
                />
                <Input
                    id="editwuomInput"
                 value="{CombinedModel>/Product/wuom}"
                 editable="false"
                /> -->
                    <HBox>
                        <Label
                            id="editHeightLabel"
                            text="Height(m)"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editprodHeightInput"
                            value="{CombinedModel>/Product/height}"
                            liveChange="onEidtHeight_LC"
                            type="Number"
                            width="5rem"
                        />
                    </HBox>
                    <HBox>
                        <Label
                            id="editLengthLabel"
                            text="Length(m)"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editproLengthInput"
                            value="{CombinedModel>/Product/length}"
                            liveChange="onEidtLenght_LC"
                            type="Number"
                            width="5rem"
                        />
                    </HBox>
                    <HBox>
                        <Label
                            id="editWidthLabel"
                            text="Width(m)"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editprodWidthInput"
                            value="{CombinedModel>/Product/width}"
                            liveChange="onEidtWidth_LC"
                            type="Number"
                            width="5rem"
                        />
                    </HBox>
                    <!-- <Label
                    id="edituomabel"
                    text="uom"
                />
                <Input
                   id="edituomInput"
                   value="{CombinedModel>/Product/uom}"
                   editable="false"
                />  -->
                    <HBox>
                        <Label
                            id="editgrossWeightLabel"
                            text="Grossweight(Kg)"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editgrossWeightInput"
                            value="{CombinedModel>/Product/grossWeight}"
                            maxLength="8"
                            liveChange="onEidtGrossWeight_LC"
                            type="Number"
                            width="5rem"
                        />
                    </HBox>
                    <HBox>
                        <Label
                            id="editnetWeightLabel"
                            text="Netweight(Kg)"
                            class="sapUiTinyMarginTop"
                            width="7rem"
                        />
                        <Input
                            id="editnetWeightInput"
                            value="{CombinedModel>/Product/netWeight}"
                            maxLength="8"
                            liveChange="onEidtNetWeight_LC"
                            type="Number"
                            width="5rem"
                        />
                    </HBox>
                </VBox>
            </form:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>